        <li role="presentation"><a data-toggle="tab" href="#fruitsTab" aria-controls="fruitsTab" role="tab">Owoce i ich przetwory</a></li>
        <li role="presentation"><a data-toggle="tab" href="#preparedTab" aria-controls="preparedTab" role="tab">Produkty gotowe</a></li>
        <li role="presentation"><a data-toggle="tab" href="#fishesTab" aria-controls="fishesTab" role="tab">Ryby i dania rybne</a></li>
        <li role="presentation"><a data-toggle="tab" href="#fatsTab" aria-controls="fatsTab" role="tab">Tłuszcze</a></li>
        <li role="presentation"><a data-toggle="tab" href="#vegetablesTab" aria-controls="vegetablesTab" role="tab">Warzywa i ich przetwory</a></li>
    </ul>

    <div class="tab-content">
        <div id="milkEggsTab" class="tab-pane fade in active" role="tabpanel">
            <table class="table table-condensed table-hover table-striped table-responsive">
                <thead>
                    <tr>
                        <th>
                            Nazwa
                        </th>
                        <th>
                            Energia
                        </th>
                        <th>
                            Białko
                        </th>
                        <th>
                            Tłuszcze
                        </th>
                        <th>
                            Cukry
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="milkEggsProducts">
                    @foreach (var item in Model.MilkEggsProducts)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Description)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Energy)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Protein)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Fat)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Carbohydrate)
                            </td>
                            <td>
                                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-success box curmudgeon btn-xs padding-5">Edytuj <i class="fa fa-pencil-square-o"></i></a>
                                <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-info box foo btn-xs padding-5">Szczegóły <i class="fa fa-info-circle"></i></a>
                                @if (!_context.DishFoodProducts.Any(x => x.FoodProductId == item.Id))
                                {
                                    <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger box bar btn-xs padding-5">Usuń <i class="fa fa-trash"></i></a>
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

